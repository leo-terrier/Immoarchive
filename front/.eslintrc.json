{
    "extends": [
        "next",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-console": "error",
        "@typescript-eslint/no-empty-function": "warn",
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["error"]
    },
    "globals": {
        "React": true,
        "google": true
    },
    "ignorePatterns": ["*cy.js", "cypress"]
}
